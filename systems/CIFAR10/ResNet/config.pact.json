{
    "description": {},
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": true
                    }
                }
            },
            "dataloader": {
                "bs": 240
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 240
            }
        },
        "cv": {
            "n_folds": 1
        }
    },
    "network": {
        "class": "ResNet",
        "kwargs": {
            "config": "ResNet20",
            "activation": "ReLU6",
            "pretrained": "/home/georgr/projects/mixed-precision-inference/ql-pact/systems/CIFAR10/ResNet/pretrained/resnet20.ckpt"
        },
        "quantize": {
            "function": "pact_recipe",
            "kwargs": {
                "config": {
                    "PACTConv2d": {
                        "pilot.0$": {
                            "n_levels": 256
                        },
                        "features.*conv1$": {
                            "n_levels": 256
                        },
                        "features.*conv[2-3]?$": {
                            "n_levels": 256
                        },
                        "features.*downsample$": {
                            "n_levels": 256
                        },
                        "kwargs": {
                            "quantize": "per_channel",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTLinear": {
                        "classifier$": {
                            "n_levels": 256
                        },
                        "kwargs": {
                            "quantize": "per_layer",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTUnsignedAct": {
                        "pilot.2$": {
                            "n_levels": 256
                        },
                        "features.*relu[1-2]$": {
                            "n_levels": 256
                        },
                        "kwargs": {
                            "init_clip": "std",
                            "noisy": false,
                            "rounding": true,
                            "learn_clip": true,
                            "tqt": true,
                            "nb_std": 3
                        }
                    },
                    "harmonize": {
                        "init_clip": "std",
                        "n_levels": [256, 256],
                        "noisy": false,
                        "rounding": true,
                        "force_out_eps": false,
                        "tqt": true,
                        "infer_outact" : true,
                        "infer_sign": true
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "Adam",
                "kwargs": {
                    "lr": 0.001
                }
            },
            "lr_sched": {
                "class": "CosineAnnealingLR",
                "kwargs": {
                    "T_max": 150
                }
            }
        },
        "n_epochs": 150,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "linear": {
                        "0": [
                            "start",
                            "verbose_on"
                        ]
                    },
                    "activation": {
                        "1": [
                            "start",
                            "verbose_on"
                        ]
                    }
                },
                "kwargs_linear": {},
                "kwargs_activation": {
                    "init_clip_hi": 6.0
                }
            }
        }
    },
    "meters": {
        "train": {
            "compute_task_statistic": true,
            "tensor_statistics": [],
            "compute_profiling_statistic": false
        },
        "valid": {
            "compute_profiling_statistic": false
        }
    },
    "experiment": {
        "target_loss": "valid",
        "ckpt_period": 1
    }
}
