{
    "description": "TQT Version of Tiny-ICCT (1-4-64-64)",
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "TransformB",
                    "kwargs": {
                        "augment": true
                    }
                }
            },
            "dataloader": {
                "bs": 512
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "TransformB",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 512
            }
        }
    },
    "network": {
        "class": "ICCT",
        "kwargs": {
            "num_layers": 1,
            "num_heads": 4,
            "embedding_dim": 64,
            "projection_dim": 64,
            "pool": "mean"
        },
        "quantize": {
            "function": "pact_recipe",
            "kwargs": {
                "config": {
                    "PACTConv2d": {
                        "tokenizer.conv_layers.0.0$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_channel",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTLinear": {
                        ".*attention.WQ.0$": {},
                        ".*attention.WK.0$": {},
                        ".*attention.WV.0$": {},
                        ".*attention.out.0$": {},
                        "transformer.layers.*.*.fn.net.0$": {},
                        "transformer.layers.*.*.fn.net.3$": {},
                        "classifier.0$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_layer",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTUnsignedAct": {
                        ".*attention.WQ.1$": {
                            "act_kind": "identity",
                            "signed": true,
                            "init_clip": "max"
                        },
                        ".*attention.WK.1$": {
                            "act_kind": "identity",
                            "signed": true,
                            "init_clip": "max"
                        },
                        ".*attention.WV.1$": {
                            "act_kind": "identity",
                            "signed": true,
                            "init_clip": "max"
                        },
                        ".*attention.AttentionMechanism.scores_activation$": {
                            "act_kind": "identity",
                            "signed": true,
                            "rounding": false,
                            "init_clip": "max"
                        },
                        ".*attention.AttentionMechanism.output_activation$": {
                            "act_kind": "identity",
                            "signed": true,
                            "rounding": false,
                            "init_clip": "max"
                        },
                        ".*attention.out.1$": {
                            "act_kind": "identity",
                            "signed": true,
                            "init_clip": "max"
                        },
                        "transformer.layers.*.*.fn.net.1$": {
                            "act_kind": "relu"
                        },
                        "tokenizer.conv_layers.0.2": {
                            "act_kind": "relu"
                        },
                        "classifier.1$": {
                            "act_kind": "relu"
                        },
                        "kwargs": {
                            "n_levels": 256,
                            "init_clip": "std",
                            "nb_std": 3,
                            "noisy": false,
                            "tqt": true,
                            "learn_clip": true,
                            "rounding": true
                        }
                    },
                    "harmonize": {
                        "n_levels": 256,
                        "init_clip": "max",
                        "noisy": false,
                        "rounding": true,
                        "force_out_eps": true,
                        "tqt": true,
                        "learn_clip": true
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "PACTSGD",
                "kwargs": {
                    "pact_decay": 0.001,
                    "lr": 0.01,
                    "momentum": 0.9
                },
                "takes_net": true
            },
            "lr_sched": {
                "class": "ReduceLROnPlateau",
                "kwargs": {}
            }
        },
        "n_epochs": 100,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "linear": {
                        "1": [
                            "verbose_on",
                            "start"
                        ],
                        "99": "freeze"
                    },
                    "activation": {
                        "1": [
                            "verbose_on",
                            "start"
                        ],
                        "99": "freeze"
                    },
                    "eps": {
                        "98": [
                            "verbose_on",
                            "start"
                        ]
                    }
                },
                "kwargs_linear": {
                    "update_every": "batch"
                },
                "kwargs_activation": {
                    "init_clip_hi": 6.0
                }
            }
        }
    },
    "meters": {
        "train": {
            "compute_profiling_statistic": true,
            "compute_task_statistic": true,
            "tensor_statistics": [
                {
                    "class": "CIFAR10InputsSnapshot",
                    "kwargs": {
                        "name": "tokenizer.conv_layers.0.0",
                        "start": 0,
                        "period": 98,
                        "n_inputs": 4,
                        "preprocessing_type": 1
                    }
                },
                {
                    "class": "OutputFeaturesSnapshot",
                    "kwargs": {
                        "name": "tokenizer.conv_layers.0.0",
                        "start": 0,
                        "period": 98,
                        "n_inputs": 4
                    }
                }
            ]
        },
        "valid": {
            "compute_profiling_statistic": true
        }
    },
    "experiment": {
        "target_loss": "valid",
        "ckpt_period": 5
    }
}