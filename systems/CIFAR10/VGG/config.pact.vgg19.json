{
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": true
                    }
                }
            },
            "dataloader": {
                "bs": 128
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 100
            }
        }
    },
    "network": {
        "class": "VGG",
        "kwargs": {
            "capacity": 1,
            "config": "VGG19",
            "use_bn_features": true,
            "use_bn_classifier": true
        },
        "quantize": {
            "function": "quantize_pact",
            "kwargs": {
                "config": {
                    "PACTConv2d": {
                        "quantize": "per_layer",
                        "init_clip": "sawb_asymm",
                        "learn_clip": false,
                        "n_levels": {
                            "features.0$": 4,
                            "features.4$": 4,
                            "features.7$": 4,
                            "features.11$": 4,
                            "features.14$": 4,
                            "features.17$": 4,
                            "features.20$": 4,
                            "features.24$": 4,
                            "features.27$": 4,
                            "features.30$": 4,
                            "features.33$": 4,
                            "features.37$": 4,
                            "features.40$": 4,
                            "features.43$": 4,
                            "features.46$": 4
                        }
                    },
                    "PACTLinear": {
                        "quantize": "per_layer",
                        "init_clip": "sawb_asymm",
                        "learn_clip": false,
                        "symm_wts": true,
                        "n_levels": {}
                    },
                    "PACTUnsignedAct": {
                        "init_clip": "std",
                        "n_levels": {
                            "pilot.2$": 256,
                            "features.2$": 4,
                            "features.6$": 4,
                            "features.9$": 4,
                            "features.13$": 4,
                            "features.16$": 4,
                            "features.19$": 4,
                            "features.22$": 4,
                            "features.26$": 4,
                            "features.29$": 4,
                            "features.32$": 4,
                            "features.35$": 4,
                            "features.39$": 4,
                            "features.42$": 4,
                            "features.45$": 4,
                            "features.48$": 256
                        },
                        "nb_std": 3
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "takes_net": false,
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "PACTSGD",
                "takes_net": true,
                "kwargs": {
                    "pact_decay": 0.001,
                    "lr": 0.1,
                    "momentum": 0.9,
                    "weight_decay": 0.0002
                }
            },
            "lr_sched": {
                "class": "HandScheduler",
                "kwargs": {
                    "schedule": {
                        "0": 1,
                        "15": 1,
                        "100": 0.1,
                        "150": 0.01,
                        "200": 0.001
                    }
                }
            }
        },
        "n_epochs": 200,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "linear": {
                        "0": [
                            "verbose_on",
                            "start"
                        ],
                        "175": "freeze"
                    },
                    "activation": {
                        "0": [
                            "verbose_on"
                        ],
                        "8": "start",
                        "175": "freeze"
                    }
                }
            }
        }
    },
    "meters": {
        "train": {
            "compute_profiling_statistic": true,
            "compute_task_statistic": true,
            "tensor_statistics": []
        },
        "valid": {
            "compute_profiling_statistic": true,
            "tensor_statistics": []
        }
    },
    "experiment": {
        "target_loss": "valid",
        "ckpt_period": 5
    }
}
