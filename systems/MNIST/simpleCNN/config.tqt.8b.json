{
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "Transform",
                    "kwargs": {
                        "augment": true
                    }
                }
            },
            "dataloader": {
                "bs": 256
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "Transform",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 256
            }
        }
    },
    "network": {
        "class": "simpleCNN",
        "kwargs": {
            "pretrained": "/scratch2/wiesep/quantlab/systems/MNIST/simpleCNN/logs/exp0002/fold0/saves/best.ckpt",
            "config": "standard",
            "capacity": 1,
            "use_bn_features": false,
            "use_bn_classifier": false
        },
        "quantize": {
            "function": "pact_recipe",
            "kwargs": {
                "config": {
                    "PACTConv2d": {
                        "pilot.0$": {},
                        "features.1$": {},
                        "features.4$": {},
                        "features.7$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_channel",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTLinear": {
                        "classifier.1$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_layer",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTUnsignedAct": {
                        "pilot.1$": {},
                        "features.2$": {},
                        "features.5$": {},
                        "features.8$": {},
                        "classifier.2$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "init_clip": "std",
                            "nb_std": 3,
                            "act_kind": "relu",
                            "noisy": false,
                            "tqt": true,
                            "learn_clip": true,
                            "rounding": true
                        }
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "PACTSGD",
                "kwargs": {
                    "pact_decay": 0.001,
                    "lr": 0.1,
                    "momentum": 0.9
                },
                "takes_net": true
            },
            "lr_sched": {
                "class": "HandScheduler",
                "kwargs": {
                    "schedule": {
                        "0": 1,
                        "10": 0.1,
                        "15": 0.01
                    }
                }
            }
        },
        "n_epochs": 21,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "activation": {
                        "0": [
                            "verbose_on",
                            "start"
                        ]
                    },
                    "linear": {
                        "0": [
                            "verbose_on",
                            "start"
                        ]
                    }
                },
                "kwargs_linear": {
                    "update_every": "batch"
                },
                "kwargs_activation": {
                    "init_clip_hi": 6.0
                }
            }
        }
    },
    "meters": {
        "train": {
            "compute_profiling_statistic": false,
            "compute_task_statistic": true,
            "tensor_statistics": [
             ]
        },
        "valid": {
            "compute_profiling_statistic": false
        }
    },
    "experiment": {
        "target_loss": "valid",
        "ckpt_period": 5
    }
}